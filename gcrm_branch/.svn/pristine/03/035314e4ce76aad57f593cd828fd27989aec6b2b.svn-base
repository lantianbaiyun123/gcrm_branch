package com.baidu.gcrm.mail;

import java.util.Set;

import com.baidu.gcrm.common.LocaleConstants;
import com.baidu.gcrm.quote.model.QuoteClashType;

public class QuoteCompleteContent {
	/**	收件人 */
	private Set<String> sendTo;
	
	/**	抄送 */
	private Set<String> cc;
	
	/**	老标杆价编号*/
	private String oldQuoteCode;
	
	/**	新标杆价编号*/
	private String newQuoteCode;
	
	/**	新增的标杆价编号 */
	private String addQuoteCode;
	/**原标杆价生成时间*/
	private String oldQuoteCreateDate;
	
	/**	原标杆价拆分后新的生效时间*/
	private String addValidDate;
	
	/**	原标杆价修改后生效时间 */
	private String oldEditValidDate;
	/**	新标杆价提交人 */
	private String submitTer;
	
	private LocaleConstants locale;
	/**冲突类型*/
    private QuoteClashType type;
    
    /**旧标杆价提交人*/
    private String operator;

	public String getOperator() {
		return operator;
	}

	public void setOperator(String operator) {
		this.operator = operator;
	}

	public Set<String> getSendTo() {
		return sendTo;
	}

	public void setSendTo(Set<String> sendTo) {
		this.sendTo = sendTo;
	}

	public Set<String> getCc() {
		return cc;
	}

	public void setCc(Set<String> cc) {
		this.cc = cc;
	}

	public String getOldQuoteCode() {
		return oldQuoteCode;
	}

	public void setOldQuoteCode(String oldQuoteCode) {
		this.oldQuoteCode = oldQuoteCode;
	}

	public String getNewQuoteCode() {
		return newQuoteCode;
	}

	public void setNewQuoteCode(String newQuoteCode) {
		this.newQuoteCode = newQuoteCode;
	}

	public String getAddQuoteCode() {
		return addQuoteCode;
	}

	public void setAddQuoteCode(String addQuoteCode) {
		this.addQuoteCode = addQuoteCode;
	}

	public String getOldQuoteCreateDate() {
		return oldQuoteCreateDate;
	}

	public void setOldQuoteCreateDate(String oldQuoteCreateDate) {
		this.oldQuoteCreateDate = oldQuoteCreateDate;
	}

	public String getAddValidDate() {
		return addValidDate;
	}

	public void setAddValidDate(String addValidDate) {
		this.addValidDate = addValidDate;
	}

	public String getOldEditValidDate() {
		return oldEditValidDate;
	}

	public void setOldEditValidDate(String oldEditValidDate) {
		this.oldEditValidDate = oldEditValidDate;
	}

	public LocaleConstants getLocale() {
		return locale;
	}

	public void setLocale(LocaleConstants locale) {
		this.locale = locale;
	}

	public QuoteClashType getType() {
		return type;
	}

	public void setType(QuoteClashType type) {
		this.type = type;
	}

	public String getSubmitTer() {
		return submitTer;
	}

	public void setSubmitTer(String submitTer) {
		this.submitTer = submitTer;
	}
    
    
}
