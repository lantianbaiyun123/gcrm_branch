package com.baidu.gcrm.common.uc;

import com.baidu.rigel.crm.rights.bo.User;

public class UpdUser extends User {
	
	private static final long serialVersionUID = 2006195005404140828L;
	
	/**
	 * 用户类型
	 */
	private String utype;
	
	/**
	 * 用户Email地址
	 */
	private String email1;
	
	/**
	 * 用户密码
	 */
	private String ucpwd;
	
	/**
	 * 座机
	 */
	private String telephone;
	
	/**
	 * 手机
	 */
	private String cellphone;
	
	/**
	 * 身份证号
	 */
	private String idcard;
	
	/**
	 * 是否允许登陆
	 */
	private Integer allowlogin;
	
	/**
	 * 性别 
	 */
	private String gender;

	public void setTelephone(String telephone) {
		setPhone(telephone);
		this.telephone = telephone;
	}

	public void setCellphone(String cellphone) {
		setMobile(cellphone);
		this.cellphone = cellphone;
	}

	public void setIdcard(String idcard) {
		setIDcard(idcard);
		this.idcard = idcard;
	}

	public String getUtype() {
		return utype;
	}

	public void setUtype(String utype) {
		this.utype = utype;
	}


	public String getEmail1() {
		return email1;
	}

	public void setEmail1(String email1) {
		this.email1 = email1;
	}

	public String getUcpwd() {
		return ucpwd;
	}

	public void setUcpwd(String ucpwd) {
		this.ucpwd = ucpwd;
	}

	public Integer getAllowlogin() {
		return allowlogin;
	}

	public void setAllowlogin(Integer allowlogin) {
		this.allowlogin = allowlogin;
	}

	public String getGender() {
		return gender;
	}

	public void setGender(String gender) {
		this.gender = gender;
	}

	public String getTelephone() {
		return telephone;
	}

	public String getCellphone() {
		return cellphone;
	}

	public String getIdcard() {
		return idcard;
	}
}

