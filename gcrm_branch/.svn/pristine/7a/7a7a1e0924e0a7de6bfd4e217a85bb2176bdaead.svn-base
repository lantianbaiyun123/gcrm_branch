package com.baidu.gcrm.ad.content.web.vo;

public class DatePeriodState {
    private boolean startChangeable = true;
    private boolean endChangeable = true;

    public boolean isStartChangeable() {
        return startChangeable;
    }

    public boolean isEndChangeable() {
        return endChangeable;
    }

    public void setStartChangeable(boolean startChangeable) {
        this.startChangeable = startChangeable;
    }

    public void setEndChangeable(boolean endChangeable) {
        this.endChangeable = endChangeable;
    }

}
