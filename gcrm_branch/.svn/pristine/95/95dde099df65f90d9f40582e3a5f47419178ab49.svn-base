package com.baidu.gcrm.opportunity.web.vo;


import java.util.List;

import com.baidu.gcrm.opportunity.model.Opportunity;
import com.baidu.gcrm.valuelistcache.model.AdvertisingPlatform;

public class OpportunityVO {
	private Opportunity opportunity;
	private String platformIds;
	List <AdvertisingPlatform> realizedAdvertisingPlatformList;
	List <AdvertisingPlatform> salesAdvertisingPlatformList;
	public Opportunity getOpportunity() {
		return opportunity;
	}
	public void setOpportunity(Opportunity opportunity) {
		this.opportunity = opportunity;
	}
	public List<AdvertisingPlatform> getRealizedAdvertisingPlatformList() {
		return realizedAdvertisingPlatformList;
	}
	public List<AdvertisingPlatform> getSalesAdvertisingPlatformList() {
		return salesAdvertisingPlatformList;
	}
	public void setRealizedAdvertisingPlatformList(
			List<AdvertisingPlatform> realizedAdvertisingPlatformList) {
		this.realizedAdvertisingPlatformList = realizedAdvertisingPlatformList;
	}
	public void setSalesAdvertisingPlatformList(
			List<AdvertisingPlatform> salesAdvertisingPlatformList) {
		this.salesAdvertisingPlatformList = salesAdvertisingPlatformList;
	}
	public String getPlatformIds() {
		return platformIds;
	}
	public void setPlatformIds(String platformIds) {
		this.platformIds = platformIds;
	}


}
