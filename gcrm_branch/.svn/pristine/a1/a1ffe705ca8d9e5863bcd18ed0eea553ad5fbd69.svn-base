package com.baidu.gcrm.ad.content.web.vo;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Transient;


@Entity
public class AdSolutionContentView4Material {
/**
 * 广告内容ID
 */
    @Id
	private Long id;
    /**
     * 广告内容编号
     */
    @Column(name="number")
    private String adSolutionContentNo;
    
    /**
     * 广告主
     */
	  @Column(name="advertisers")
	private String advertisers;
	  /**
	   * 广告内容
	   */
	  @Column(name="description")
	  private String description;
	  /**
	   * 投放时间
	   */
	  @Column(name="period_description")
	  private String throwTime;
	  /**
	   * 投放位置(根据以下几个ID重新拼接投放位置信息）
	   */
	  @Transient
	  private String throwPlace;
	  /**
	   * 投放平台ID
	   */
	  @Column(name="product_id")
	  private Long throwPlatformId;
	  /**
	   * 投放站点ID
	   */ 
	  @Column(name="site_id")
	  private Long throwSiteId;
	  /**
	   * 投放频道ID
	   */  
	  @Column(name="channel_id")
	  private Long throwChannelId;
	  /**
	   * 投放区域ID
	   */  
	  @Column(name="area_id")
	  private Long throwAreaId;
	  /**
	   * 投放位置ID
	   */  
	  @Column(name="position_id")
	  private Long throwPositionId;
	  /**
	   * 物料申请单ID(当需催办时，不为空）
	   */
	  @Column(name="material_apply_id")
	  private Long materialApplyId;
	  
    public Long getId() {
        return id;
    }
    public void setId(Long id) {
        this.id = id;
    }
    public String getAdSolutionContentNo() {
        return adSolutionContentNo;
    }
    public void setAdSolutionContentNo(String adSolutionContentNo) {
        this.adSolutionContentNo = adSolutionContentNo;
    }
    public String getAdvertisers() {
        return advertisers;
    }
    public void setAdvertisers(String advertisers) {
        this.advertisers = advertisers;
    }
    public String getDescription() {
        return description;
    }
    public void setDescription(String description) {
        this.description = description;
    }
    public String getThrowTime() {
        return throwTime;
    }
    public void setThrowTime(String throwTime) {
        this.throwTime = throwTime;
    }
    public String getThrowPlace() {
        return throwPlace;
    }
    public void setThrowPlace(String throwPlace) {
        this.throwPlace = throwPlace;
    }
    public Long getThrowPlatformId() {
        return throwPlatformId;
    }
    public void setThrowPlatformId(Long throwPlatformId) {
        this.throwPlatformId = throwPlatformId;
    }
    public Long getThrowSiteId() {
        return throwSiteId;
    }
    public void setThrowSiteId(Long throwSiteId) {
        this.throwSiteId = throwSiteId;
    }
    public Long getThrowChannelId() {
        return throwChannelId;
    }
    public void setThrowChannelId(Long throwChannelId) {
        this.throwChannelId = throwChannelId;
    }
    public Long getThrowAreaId() {
        return throwAreaId;
    }
    public void setThrowAreaId(Long throwAreaId) {
        this.throwAreaId = throwAreaId;
    }
    public Long getThrowPositionId() {
        return throwPositionId;
    }
    public void setThrowPositionId(Long throwPositionId) {
        this.throwPositionId = throwPositionId;
    }
    public Long getMaterialApplyId() {
        return materialApplyId;
    }
    public void setMaterialApplyId(Long materialApplyId) {
        this.materialApplyId = materialApplyId;
    }
	  
  
}
