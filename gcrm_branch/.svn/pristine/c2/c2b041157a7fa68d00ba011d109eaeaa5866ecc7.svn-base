package com.baidu.gcrm.mail;

import com.baidu.gcrm.common.LocaleConstants;

public class PositionStockContent extends BaseMailContent {
    
    private String receiverName;
    
    private String timeRange;
    
    private String adSolutionOperator;
    
    private String adSolutionNumber;
    
    private String positionName;
    
    private String influencedAdSolutionNumber;
    
    private LocaleConstants locale = LocaleConstants.en_US;
    
    public String getReceiverName() {
        return receiverName;
    }

    public void setReceiverName(String receiverName) {
        this.receiverName = receiverName;
    }
    
    public String getAdSolutionOperator() {
        return adSolutionOperator;
    }

    public void setAdSolutionOperator(String adSolutionOperator) {
        this.adSolutionOperator = adSolutionOperator;
    }

    public String getAdSolutionNumber() {
        return adSolutionNumber;
    }

    public void setAdSolutionNumber(String adSolutionNumber) {
        this.adSolutionNumber = adSolutionNumber;
    }

    public String getPositionName() {
        return positionName;
    }

    public void setPositionName(String positionName) {
        this.positionName = positionName;
    }

    public String getInfluencedAdSolutionNumber() {
        return influencedAdSolutionNumber;
    }

    public void setInfluencedAdSolutionNumber(String influencedAdSolutionNumber) {
        this.influencedAdSolutionNumber = influencedAdSolutionNumber;
    }

    public LocaleConstants getLocale() {
        return locale;
    }

    public void setLocale(LocaleConstants locale) {
        this.locale = locale;
    }

    public String getTimeRange() {
        return timeRange;
    }

    public void setTimeRange(String timeRange) {
        this.timeRange = timeRange;
    }
    


}
