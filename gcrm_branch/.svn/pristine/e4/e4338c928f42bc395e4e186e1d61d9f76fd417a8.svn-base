package com.baidu.gcrm.publish.web.vo;

import java.util.Date;

public class PublishDateVO {
	private Long id;
	
	private String publishNumber;
	
	private Date planStart;
	
	private Date planEnd;
	
	private Date acturalStart;
	
	private Date acturalEnd;
	
	private String startOperator;
	
	private String endOperator;
	
	private boolean showOnline = false;//显示上线按钮
	private boolean countOnline = false;//计算上线按钮
	
	private boolean showOffline = false;//显示下线按钮
	private boolean countOffline = false;//计算下线按钮
	
	private boolean showForceOnline = false;//是否显示强制上线
	private boolean countForceOnline = false;//计算强制上线
	
	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public String getPublishNumber() {
		return publishNumber;
	}

	public void setPublishNumber(String publishNumber) {
		this.publishNumber = publishNumber;
	}

	public Date getPlanStart() {
		return planStart;
	}

	public void setPlanStart(Date planStart) {
		this.planStart = planStart;
	}

	public Date getPlanEnd() {
		return planEnd;
	}

	public void setPlanEnd(Date planEnd) {
		this.planEnd = planEnd;
	}

	public Date getActuralStart() {
		return acturalStart;
	}

	public void setActuralStart(Date acturalStart) {
		this.acturalStart = acturalStart;
	}

	public Date getActuralEnd() {
		return acturalEnd;
	}

	public void setActuralEnd(Date acturalEnd) {
		this.acturalEnd = acturalEnd;
	}

	public String getStartOperator() {
		return startOperator;
	}

	public void setStartOperator(String startOperator) {
		this.startOperator = startOperator;
	}

	public String getEndOperator() {
		return endOperator;
	}

	public void setEndOperator(String endOperator) {
		this.endOperator = endOperator;
	}

	public boolean isShowOnline() {
		return showOnline;
	}

	public void setShowOnline(boolean showOnline) {
		this.showOnline = showOnline;
	}

	public boolean isShowOffline() {
		return showOffline;
	}

	public void setShowOffline(boolean showOffline) {
		this.showOffline = showOffline;
	}

	public boolean isShowForceOnline() {
		return showForceOnline;
	}

	public void setShowForceOnline(boolean showForceOnline) {
		this.showForceOnline = showForceOnline;
	}

	public boolean isCountOnline() {
		return countOnline;
	}

	public void setCountOnline(boolean countOnline) {
		this.countOnline = countOnline;
	}

	public boolean isCountOffline() {
		return countOffline;
	}

	public void setCountOffline(boolean countOffline) {
		this.countOffline = countOffline;
	}

	public boolean isCountForceOnline() {
		return countForceOnline;
	}

	public void setCountForceOnline(boolean countForceOnline) {
		this.countForceOnline = countForceOnline;
	}
}
