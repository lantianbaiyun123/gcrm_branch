insert into g_ad_platform_site_relation 
(ad_platform_id,site_id,status,create_time,create_operator,last_update_time,last_update_operator)
select  ad_platform_id,id,1,now(),1000,now(),1000 from g_site where ad_platform_id is not null;

update g_i18n set key_name=concat('g_position.',SUBSTRING(key_name,10)) 
where key_name like 'position.%';
update g_i18n set key_name=concat('g_site.',SUBSTRING(key_name,6)) 
where key_name like 'site.%';

update g_i18n set key_extra_value=null where key_name like 'g_position.%';

update g_i18n t,g_position p,g_i18n l set t.key_extra_value=l.key_value 
    where t.key_name=concat('g_position.',p.id) and t.locale='zh_CN' 
    and l.key_name=concat('g_advertising_platform.',p.ad_platform_id) and l.locale='zh_CN'
    and p.type=0;
 
update g_i18n t,g_position p,g_i18n l set t.key_extra_value=l.key_value 
	where t.key_name=concat('g_position.',p.id) and t.locale='en_US' 
    and l.key_name=concat('g_advertising_platform.',p.ad_platform_id) and l.locale='en_US'
    and p.type=0;


update g_i18n t,g_position p,g_i18n l set t.key_extra_value=concat(t.key_extra_value,'>',l.key_value)
    where t.key_name=concat('g_position.',p.id) and t.locale='zh_CN' 
    and l.key_name=concat('g_site.',p.site_id) and l.locale='zh_CN' 
    and p.type=0;
 
update g_i18n t,g_position p,g_i18n l set t.key_extra_value=concat(t.key_extra_value,'>',l.key_value)
    where t.key_name=concat('g_position.',p.id) and t.locale='en_US' 
    and l.key_name=concat('g_site.',p.site_id) and l.locale='en_US'
    and p.type=0;
 
update g_i18n t,g_position p,g_i18n l set t.key_extra_value=concat(t.key_extra_value,'>',l.key_value)
    where t.key_name=concat('g_position.',p.id) and t.locale='zh_CN' 
    and l.key_name=concat('g_position.',p.id) and l.locale='zh_CN'
    and p.type=0;
 
update g_i18n t,g_position p,g_i18n l set t.key_extra_value=concat(t.key_extra_value,'>',l.key_value)
    where t.key_name=concat('g_position.',p.id) and t.locale='en_US' 
    and l.key_name=concat('g_position.',p.id) and l.locale='en_US'
    and p.type=0;
 
update g_i18n t,g_position p,g_i18n l set t.key_extra_value=concat(l.key_extra_value,'>',t.key_value)
    where t.key_name=concat('g_position.',p.id) and t.locale='zh_CN' 
    and l.key_name=concat('g_position.',p.parent_id) and l.locale='zh_CN'
    and p.type=1;
 
update g_i18n t,g_position p,g_i18n l set t.key_extra_value=concat(l.key_extra_value,'>',t.key_value)
    where t.key_name=concat('g_position.',p.id) and t.locale='en_US'
    and l.key_name=concat('g_position.',p.parent_id) and l.locale='en_US'
    and p.type=1;
 
update g_i18n t,g_position p,g_i18n l set t.key_extra_value=concat(l.key_extra_value,'>',t.key_value)
    where t.key_name=concat('g_position.',p.id) and t.locale='zh_CN' 
    and l.key_name=concat('g_position.',p.parent_id) and l.locale='zh_CN'
    and p.type=2;
 
update g_i18n t,g_position p,g_i18n l set t.key_extra_value=concat(l.key_extra_value,'>',t.key_value)
    where t.key_name=concat('g_position.',p.id) and t.locale='en_US' 
    and l.key_name=concat('g_position.',p.parent_id) and l.locale='en_US'
    and p.type=2;
 
 ALTER TABLE `g_site`  DROP COLUMN `ad_platform_id`;
 
 update g_advertising_platform set status=1 where status is null;
 
insert into g_site (id,code) values (-1,'CN');
update g_site set id=0 where id=-1;

